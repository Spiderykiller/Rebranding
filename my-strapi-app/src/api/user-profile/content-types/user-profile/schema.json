{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "UserProfile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "clerkUserid": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "role": {
      "type": "enumeration",
      "default": "user",
      "enum": [
        "user",
        "admin"
      ]
    },
    "isActive": {
      "type": "boolean",
      "default": true
    }
  }
}
